{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "/.cache/",
    "/.cspell.json",
    "/.cspellcache",
    "/.git",
    "/.project-words.txt",
    "/bindings/cpp/doc/header.html",
    "/build/",
    "/doc/book/book/",
    "/target/",
    "*.lock",
    "*.svg",
    "/LICENSE-APACHE",
    "/LICENSE-MIT",
  ],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./.project-words.txt",
      "addWords": true
    },
    {
      "name": "dddmp",
      "words": [
        "auxids",
        "nnodes",
        "nroots",
        "nsuppvars",
        "nvars",
        "permid",
        "permids",
        "orderedvarnames",
        "rootids",
        "rootnames",
        "suppvar",
        "suppvarnames",
        "varinfo",
        "varnames",
      ]
    }
  ],
  "dictionaries": [
    "project-words"
  ],
}
